{
  "name": "environmentmanager",
  "version": "6.22.2",
  "description": "EnvironmentManager Server",
  "homepage": "https://trainline.github.io/environment-manager/",
  "bugs": {
    "url": "https://github.com/trainline/environment-manager/issues",
    "email": "platform.development@thetrainline.com"
  },
  "config": {
    "git-tag-version": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/trainline/environment-manager.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "gulp build --gulpfile gulpfile.js -p -o build",
    "start": "node index.js",
    "dev": "nodemon index.js",
    "lint": "eslint . --ext .js --quiet",
    "lint-rule": "eslint . --ext .js --quiet --parser-options {ecmaVersion:6} --rule ",
    "test": "npm run lint -s && npm run unit-test -s",
    "test-watch": "mocha --watch --recursive ./test --EM_PROFILE test/test-profile.json",
    "test-ci": "npm test -- --reporter mocha-teamcity-reporter --timeout 4000",
    "unit-test": "mocha --recursive ./test --EM_PROFILE test/test-profile.json",
    "mocha": "mocha"
  },
  "author": {
    "name": "Platform Development",
    "email": "platform.development@thetrainline.com"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "activedirectory": "~0.7.2",
    "ajv": "~4.10.0",
    "app-module-path": "~2.1.0",
    "async": "~2.1.4",
    "aws-sdk": "~2.7.10",
    "bluebird": "~3.4.6",
    "body-parser": "~1.15.2",
    "cache-manager": "~2.4.0",
    "co": "~4.6.0",
    "compression": "~1.6.2",
    "consul": "~0.27.0",
    "cookie-parser": "~1.4.3",
    "deep-freeze-strict": "^1.1.1",
    "es6-template-strings": "~2.0.1",
    "exp-leader-election": "^0.2.2",
    "express": "~4.14.0",
    "express-request-id": "~1.3.0",
    "glob-intersection": "~0.1.3",
    "ioredis": "~2.5.0",
    "js-joda": "~1.1.17",
    "js-yaml": "~3.7.0",
    "jsonwebtoken": "^8.2.1",
    "later": "~1.2.0",
    "lodash": "^4.17.10",
    "md5": "^2.2.1",
    "moment": "^2.22.1",
    "moment-timezone": "^0.5.16",
    "ms": "~0.7.2",
    "nconf": "^0.8.5",
    "newrelic": "~1.36.2",
    "pino": "^4.16.1",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "retry": "~0.10.0",
    "rewire": "^2.5.2",
    "semver": "~5.3.0",
    "semver-sort": "0.0.4",
    "source-map-support": "^0.5.5",
    "swagger-tools": "^0.10.3",
    "uuid": "^3.2.1",
    "winston": "~2.3.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb-base": "^10.0.1",
    "eslint-plugin-dependencies": "^2.4.0",
    "eslint-plugin-import": "^2.11.0",
    "mocha": "^3.5.3",
    "mocha-duplicate-reporter": "^0.2.1",
    "mocha-teamcity-reporter": "^1.1.1",
    "mock-express-request": "^0.2.2",
    "mock-express-response": "^0.2.2",
    "nodemon": "^1.17.3",
    "proxyquire": "^1.7.10",
    "should": "^11.1.1",
    "sinon": "^1.17.6"
  },
  "engines": {
    "node": ">=6.7.0",
    "npm": ">=3.10.3"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "jshintConfig": {
    "esversion": 6,
    "globals": {
      "setInterval": true,
      "Buffer": true,
      "console": true,
      "process": true,
      "global": true,
      "__dirname": true,
      "require": true,
      "module": true,
      "describe": true,
      "before": true,
      "after": true,
      "it": true
    },
    "undef": true,
    "strict": "global"
  }
}
